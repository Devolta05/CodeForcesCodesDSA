#include <iostream>
 
using namespace std;
 
int main()
{
    int n,h,a[1000],s=0;
    cin >> n >> h;
    if ( n>=1 && n<=1000 && h>=1 && h<=1000){
        for(int i=0;i<n && n<=1000;i++){
            cin >> a[i];
            if(a[i] > h)
                s+=2;
            else
                s+=1;
        }
        cout << s << endl;
    }
    return 0;
}
